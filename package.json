{
  "version": "1.0.0",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:vite": "TAILWIND_MODE=watch vite",
    "dev:css": "NODE_ENV=development tailwindcss ./src/styles/tailwind.css -o ./src/styles/index.css -w",
    "build": "vite build",
    "format": "prettier --write src --ignore-path ./.gitignore",
    "lint": "NODE_ENV=development eslint --ext .jsx src",
    "serve": "vite preview",
    "release": "standard-version"
  },
  "dependencies": {
    "@agrc/layer-selector": "^4.0.5",
    "@arcgis/core": "^4.19.3",
    "proj4": "^2.7.4",
    "query-string": "^7.0.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-error-boundary": "^3.1.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@nabla/vite-plugin-eslint": "^1.2.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.2.6",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.0",
    "tailwindcss": "^2.2.4",
    "vite": "^2.3.8",
    "vite-react-jsx": "^1.1.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "@babel/eslint-parser",
    "rules": {
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off"
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "prettier"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  }
}
