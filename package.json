{
  "name": "chalkdust",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "dev:function": "nodemon --inspect=0.0.0.0 --watch function/index.js node_modules/@google-cloud/functions-framework --target=sendgrid",
    "build": "vite build",
    "format": "prettier --write src --ignore-path ./.gitignore",
    "lint": "NODE_ENV=development eslint --ext .jsx src",
    "serve": "vite preview",
    "release": "standard-version"
  },
  "dependencies": {
    "@agrc/layer-selector": "^4.0.5",
    "@arcgis/core": "^4.19.3",
    "proj4": "^2.7.4",
    "query-string": "^7.0.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-error-boundary": "^3.1.3",
    "@sendgrid/client": "^7.4.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@nabla/vite-plugin-eslint": "^1.3.1",
    "@vitejs/plugin-react-refresh": "^1.3.5",
    "autoprefixer": "^10.2.6",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "postcss": "^8.3.5",
    "@google-cloud/functions-framework": "^1.9.0",
    "dotenv": "^10.0.0",
    "http-server": "^0.12.3",
    "nodemon": "^2.0.9",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.0",
    "tailwindcss": "^2.2.4",
    "vite": "^2.4.1",
    "vite-react-jsx": "^1.1.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off"
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  }
}
